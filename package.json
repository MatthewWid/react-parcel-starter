{
	"name": "react-parcel-starter",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"start": "parcel ./src/index.html -d ./build/",
		"build": "rm -rf ./build/ && parcel build ./src/index.html -d /build/ --public-url /",
		"deploy": "gh-pages -d ./build/"
	},
	"dependencies": {
		"react": "^16.12.0",
		"react-dom": "^16.12.0"
	},
	"devDependencies": {
		"@babel/core": "^7.7.4",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"gh-pages": "^2.1.1",
		"parcel-bundler": "^1.12.4",
		"sass": "^1.23.7"
	}
}
